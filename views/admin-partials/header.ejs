        <li class="nav-item">
          <a href="/" class="nav-link py-3 border-bottom rounded-0" aria-current="page" title="Home" data-bs-toggle="tooltip" data-bs-placement="right">
            <svg class="bi pe-none" width="24" height="24" role="img" aria-label="Home"><use xlink:href="#home"/></svg>
          </a>
        </li>
        <li>
          <a href="/admin" class="nav-link py-3 border-bottom rounded-0 <%= title === "Dashboard" ? "active" : "" %>" title="Dashboard" data-bs-toggle="tooltip" data-bs-placement="right">
